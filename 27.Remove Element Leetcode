class Solution {
    public int removeElement(int[] nums, int val) {
        int count=0;
        ArrayList<Integer> li=new ArrayList<>();
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]==val){
                count++;
            }else{
                li.add(nums[i]);
            }
             
        }
        for(int i=0;i<li.size();i++)
        {
            nums[i]=li.get(i);
        }
       return nums.length-count;
    }
}
